{
  "name": "zlog-web",
  "version": "1.0.1",
  "description": "A tiny JavaScript debugging utility, Works in web browsers.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^1.0.3",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@swc/core": "^1.3.99",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.9.0",
    "@typescript-eslint/typescript-estree": "^5.9.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.6.0",
    "eslint-config-ali": "^13.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "query-string": "^7.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-swc3": "^0.10.4",
    "tiny-glob": "^0.2.9",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "dev": "cross-env BUILD_ENV=pre npx rollup -c ./rollup.config.mjs -w --no-watch.clearScreen",
    "build": "npm run build:clean && cross-env BUILD_ENV=prod rollup -c ./rollup.config.mjs",
    "build:clean": "del-cli dist",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asins/zlog.git"
  },
  "keywords": [
    "console",
    "debug",
    "TypeScript"
